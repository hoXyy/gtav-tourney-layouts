{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "default": "AlexHonix"
    },
    "src": {
      "type": "string",
      "default": "AlexHonix"
    },
    "pb": {
      "type": "string"
    },
    "hasAvatar": {
      "type": "boolean"
    },
    "avatar": {
      "type": "string"
    },
    "finishTime": {
      "type": "string"
    }
  },
  "required": ["name", "src"]
}
